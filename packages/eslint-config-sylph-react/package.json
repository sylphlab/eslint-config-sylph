{
  "name": "@sylphlab/eslint-config-sylph-react",
  "version": "4.0.5",
  "description": "React ESLint Flat Config for SylphLab projects.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist .turbo",
    "lint": "eslint . --cache --max-warnings=0",
    "lint:fix": "eslint . --fix --cache",
    "validate": "pnpm run lint && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphlab/typescript.git",
    "directory": "packages/eslint-config-sylph-react"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "flat config",
    "typescript",
    "react",
    "sylphlab"
  ],
  "author": "SylphLab <hi@sylphlab.com> (https://sylphlab.com) (https://sylphlab.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylphlab/typescript/issues"
  },
  "homepage": "https://github.com/sylphlab/typescript/tree/main/packages/eslint-config-sylph-react#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sylphlab/eslint-config-sylph": "workspace:*",
    "@sylphlab/typescript-config": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "jiti": "^2.4.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "peerDependencies": {
    "@eslint/js": "^9.24.0",
    "@sylphlab/eslint-config-sylph": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "react": ">=19.1.0",
    "typescript": "^5.8.3"
  }
}