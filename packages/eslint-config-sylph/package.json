{
  "name": "@sylphlab/eslint-config-sylph",
  "version": "3.3.0",
  "description": "Strict, AI-optimized ESLint Flat Config for modern TypeScript projects (v9+, TS, Unicorn, Functional, Security, Sonar, Import, Promise, Regexp, Prettier). Core of all SylphLab TypeScript projects.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist .turbo",
    "lint": "eslint . --cache --max-warnings=0",
    "lint:fix": "eslint . --fix --cache",
    "validate": "pnpm run lint && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphlab/typescript.git",
    "directory": "packages/eslint-config-sylph"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "flat config",
    "typescript",
    "ai",
    "strict",
    "prettier",
    "unicorn",
    "functional",
    "security",
    "sonarjs",
    "import",
    "promise",
    "regexp",
    "sylph",
    "sylphlab"
  ],
  "author": "SylphLab <hi@sylphlab.com> (https://sylphlab.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylphlab/typescript/issues"
  },
  "homepage": "https://github.com/sylphlab/typescript/tree/main/packages/eslint-config-sylph#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "globals": "^16.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@sylphlab/typescript-config": "workspace:*",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "peerDependencies": {
    "@eslint/js": "^9.24.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^58.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}