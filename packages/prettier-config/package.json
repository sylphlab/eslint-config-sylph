{
  "name": "@sylphlab/prettier-config",
  "version": "0.2.3",
  "description": "Shared Prettier configuration for SylphLab projects",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "prettier",
    "prettier-config",
    "sylphlab",
    "formatting",
    "code-style",
    "standard",
    "prettier-plugin",
    "organize-imports",
    "packagejson"
  ],
  "files": [
    "index.js",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint . --ext .js"
  },
  "dependencies": {
    "prettier-plugin-packagejson": "^2.4.11",
    "prettier-plugin-organize-imports": "^3.2.4"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "devDependencies": {
    "prettier": "^3.2.0"
  }
}